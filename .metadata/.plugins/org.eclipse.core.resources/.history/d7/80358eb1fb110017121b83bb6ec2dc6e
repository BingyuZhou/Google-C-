#include <iostream>
#include <string>
using namespace std;


int main(){
	string code;
	double price;
	int flag = 1;
	int num_hand;

	do{
		cout << "Bood code: "; cin >> code;
		cout << "Single price: "; cin >> price;
		cout << "Num on hand: "; cin >> num_hand;
	}while (flag == 1);

}


